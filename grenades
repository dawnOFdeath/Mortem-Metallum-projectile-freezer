print("sudz")
local plr = game.Players.LocalPlayer
local bc = plr.Backpack
local char = plr.Character
local gren
if bc:FindFirstChild("Grenade") then
   bc.Grenade.Parent = char
end
task.wait()
if char:FindFirstChild("Grenade") then
   for spam = 1, 1000 do
      Grenade.start:FireServer()
      wait(0.000000000001)
   end
   for spam = 1, 1000 do
      gren.throw:FireServer()
      wait(0.000000000001)
   end
   print("frozen")
end
